# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1

# Stack Configuration
STACK_NAME=co-intelligence
EKS_CLUSTER_NAME=co-intelligence-cluster

# Database Configuration
DB_USERNAME=cointelligence
DB_PASSWORD=SecurePass123!
DATABASE_URL=postgres://cointelligence:SecurePass123!@your-rds-endpoint.rds.amazonaws.com:5432/postgres?ssl=require

# Application Secrets
SECRET_KEY=your-secret-key-min-32-chars-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AI API Keys (Optional - can be empty)
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-2.5-flash-lite
GROQ_API_KEY=your-groq-api-key

# S3 Configuration
S3_BUCKET_NAME=co-intelligence-bucket

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
